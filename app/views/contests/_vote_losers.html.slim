- if vote_losers.any?
  div class="#{position}-part"
    .subheadline Побеждены
    .entries
      - vote_losers.each do |entry|
        a.bubbled href="#{url_for(entry)}" data-remote="true" data-offset-top="20"
          img src="#{entry.image.url(:x64)}" alt="#{localized_name entry}" title="#{localized_name entry}"
